{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "/home/stef/itsrobel-stable",
    "strictNullChecks": false,
    "noImplicitAny": false,
    "paths": {
      "@/utils": [
        "src/utils/index.ts"
      ],
      "@/components/*": [
        "src/components/*.astro",
        "src/components/*.tsx"
      ],
      "@/layouts/*": [
        "src/layouts/*.astro"
      ]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  },
  "include": [
    "src",
    "*.ts"
  ]
}